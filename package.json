{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "yarn run clean && yarn run build-babel",
    "start": "yarn run build && node ./build/bin/server.js",
    "test": "nyc --reporter=html --reporter=text ./node_modules/mocha/bin/mocha --require @babel/polyfill --require @babel/register './tests/**/*.spec.js' --timeout 10000 --exit"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "app-root-path": "^2.2.1",
    "axios": "^0.21.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "firebase-admin": "8.6.0",
    "helmet": "^3.21.0",
    "http-errors": "~1.6.3",
    "mocha": "^6.2.1",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "pug": "2.0.0-beta11",
    "redis": "^2.8.0",
    "redux": "^4.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
